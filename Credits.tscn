script/source = "extends Node2D

#var music_player = FLMusicLib.new();
onready var music_player = Globals.ReinaAudioPlayer.new(self)

func _ready():
	$Sprite.Cover()
	#music_player.load_song(\"Credits\",\"Rockman 4MI.nsf\",104)
	music_player.load_song(\"Credits\",\"DMG-ADNJ-JPN.gbs\",3,8)
	#music_player.load_song(\"Credits\",\"Metroid.nsf\",11)
	
	
	$c1.rect_position=Vector2(
		Globals.gameResolution.x/2-$c1.rect_size.x/2,
		Globals.gameResolution.y/2-$c1.rect_size.y/2
	)
	#$c2.modulate.a=0
	#var tween2 = $Tween
	#tween2.interpolate_property($c1, 'modulate',
	#Color(0,0,0,0), Color(1,1,1,1), .5, Tween.TRANS_QUAD, Tween.EASE_OUT);
	#tween2.start();
	
	$c1.set_modulate(Color(1,1,1,0))
	var seq := TweenSequence.new(get_tree())
	#seq.append($c1,'modulate',Color(1,1,1,0),1) #sleep 1
	seq.append($c1,'modulate',Color(1,1,1,1),1).set_trans(Tween.TRANS_LINEAR)
	seq.parallel().append($Sprite,\"modulate\",Color(.5,.5,.5,1),.3)
	seq.append($c1,'modulate',Color(1,1,1,1),3) #sleep 1
	seq.append($c1,'modulate',Color(1,1,1,0),1).set_trans(Tween.TRANS_LINEAR)
	
	#print(Globals.gameResolution.y-$c2.rect_size.y)
	seq.append($c2,'rect_position:y',-$c2.rect_size.y,30)
	seq.append($Sprite,'modulate',Color(0,0,0,1),1)
	seq.append_callback(self,'screen_out')
	
	#var seq2 := TweenSequence.new(get_tree())
	#seq2.append($c2,'modulate',Color(1,1,1,1),4) #sleep 1
	#seq2.append($c2,'rect_position',Vector2(Globals.gameResolution.x/2-$c2.rect_size.x/2,0),10).set_trans(Tween.TRANS_LINEAR).from_current()

func screen_out():
# warning-ignore:return_value_discarded
	get_tree().change_scene(\"res://TitleScreenV2.tscn\")

func _input(_event):
	if Input.is_action_just_pressed(\"ui_pause\"):
		screen_out()

