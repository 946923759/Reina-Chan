script/source = "extends Sprite

export (int,-1,15) var teleporter_ID = -1
export (int,\"In\",\"Out\") var teleport_type = 1
# If this is true and type is set to In, it will only work 
# if bitflag in CheckpointPlayerUtils is set
export (bool) var require_bitflag_to_activate = false
export (int,\"Blink\",\"Stop Animation\") var when_bitflag_is_true = 0
#export (bool) var blink_based_on_bitflag = false
#export (bool) var 
#export (int,\"Disabled\",\"Enabled\")


var drawing:bool = false
var time:float = 0

func _draw():
	if drawing:
		draw_texture_rect_region(
			self.texture,
			Rect2(0,0,32,16),
			Rect2(32*frame,0,32,16)
		)

func _process(delta):
	time+=delta
	if time>=.5:
		time-=.5
		drawing = !drawing
		update()
	if when_bitflag_is_true==1 && teleporter_ID >= 0:
		if CheckpointPlayerStats.temporaryStageStats & 1<<teleporter_ID:
			set_inactive()
			#return

func set_inactive():
	set_process(false)
	drawing=false
	update()
"

