script/source = "extends Node2D
tool

export(int) var SPEED = 50

onready var line = $Sprite2
func _process(delta):
	var y = line.region_rect.position.y
	y-=SPEED*delta
	if y <= 0:
		y=225
	line.region_rect.position.y=y
"

