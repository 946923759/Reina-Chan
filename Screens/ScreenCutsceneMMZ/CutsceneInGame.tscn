script/source = "extends Sprite

#var blinky_is_active:bool=false

func _ready():
	set_process(false)

var timer:float=0.0
func _process(delta):
	timer+=delta
	visible=(timer > .5)
	if timer>=1:
		timer=0
		

func show():
	#timer=1
	set_process(true)
	
func hide():
	set_process(false)
	visible=false
	timer=.5
"

